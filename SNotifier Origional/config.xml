        <icon height="40" src="www/img/ios-images/AppIcon.appiconset/Icon-App-40x40@2x.png" width="40" />
        <icon height="80" src="www/img/ios-images/AppIcon.appiconset/Icon-App-40x40@2x.png" width="80" />
        <icon height="57" src="www/img/ios-images/AppIcon.appiconset/ItunesArtwork@2x.png" width="57" />
        <icon height="114" src="www/img/ios-images/AppIcon.appiconset/ItunesArtwork@2x.png" width="114" />
        <icon height="72" src="www/img/ios-images/AppIcon.appiconset/Icon-App-72x72@2x.png" width="72" />
        <icon height="144" src="www/img/ios-images/AppIcon.appiconset/Icon-App-72x72@2x.png" width="144" />
        <icon height="167" src="www/img/ios-images/AppIcon.appiconset/Icon-App-72x72@2x.png" width="167" />
