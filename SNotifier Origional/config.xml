        <icon density="xxxhdpi" src="www/img/android-images/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <icon height="180" src="www/img/ios-images/AppIcon.appiconset/Icon-App-60x60@3x.png" width="180" />
        <icon height="60" src="www/img/ios-images/AppIcon.appiconset/Icon-App-60x60@2x.png" width="60" />
