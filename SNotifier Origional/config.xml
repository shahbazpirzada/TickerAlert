        <icon height="50" src="www/img/ios-images/AppIcon.appiconset/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="www/img/ios-images/AppIcon.appiconset/Icon-Small-50x50@1x.png" width="100" />
        <icon height="167" src="www/img/ios-images/AppIcon.appiconset/Icon-App-83.5x83.5@2x.png" width="167" />
