    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <allow-navigation href="localhost:8100" />
    <allow-navigation href="http://192.168.1.29:8100" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="phonegap-plugin-push" spec="^1.10.5">
        <variable name="SENDER_ID" value="647987604288" />
    </plugin>
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="^0.8.5" />
    <plugin name="cordova-plugin-background-mode" spec="^0.7.2" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.0" />
    <allow-navigation href="http://192.168.1.11:8100" />
    <allow-navigation href="http://192.168.128.2:8100" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
